      "cauHoi": "<table>\n        <tbody>\n                <tr>\n                        <td><b>Maria:</b></td>\n                        <td>This<b> (1) </b>______ Peter.</td>\n                </tr>\n                <tr>\n                        <td><b>John:</b></td>\n                        <td>Nice to<b> (2)</b> ______ you, Pedro.</td>\n                </tr>\n                <tr>\n                        <td><b>Maria:</b></td>\n                        <td>No. His<b> (3)</b> ______ isn't Pedro. His name is Peter.</td>\n                </tr>\n                <tr>\n                        <td><b>John:</b></td>\n                        <td>Peter! <b>(4)</b> ______. Nice to meet you!</td>\n                </tr>\n        </tbody>\n</table>",
      "yNghia": "<table>\n        <tbody>\n                <tr>\n                        <td><b>Maria:</b></td>\n                        <td>This <b>(1)</b> <span style=\"color:red\"><b>is </b></span> Peter.</td>\n                </tr>\n                <tr>\n                        <td> </td>\n                        <td><i>Đây <span style=\"color:red\">là </span> Peter.</i></td>\n                </tr>\n                <tr>\n                        <td><b>John:</b></td>\n                        <td>Nice to <b>(2)</b> <span style=\"color:red\"><b>meet</b></span>  you, Pedro.</td>\n                </tr>\n                <tr>\n                        <td> </td>\n                        <td><i>Rất vui khi được <span style=\"color:red\">gặp </span> bạn, Pedro.</i></td>\n                </tr>\n                <tr>\n                        <td><b>Maria:</b></td>\n                        <td>No. His <b>(3)</b> <span style=\"color:red\"><b>name </b></span> isn't Pedro. His name is Peter.</td>\n                </tr>\n                <tr>\n                        <td> </td>\n                        <td><i>Không. <span style=\"color:red\">Tên </span> của anh ấy không phải là Pedro. Tên của anh ấy là Peter.</i></td>\n                </tr>\n                <tr>\n                        <td><b>John:</b></td>\n                        <td>Peter! <b>(4)</b> <b><span style=\"color:red\">Sorry</span> </b>. Nice to meet you!</td>\n                </tr>\n                <tr>\n                        <td> </td>\n                        <td><i>Peter! <span style=\"color:red\">Xin lỗi</span> . Rất vui khi được gặp bạn.</i></td>\n                </tr>\n        </tbody>\n</table>",
