      "cauHoi": "Mom",
      "danhSachDapAn": [
        {
          "_id": {
            "$oid": "d478325c-a5b7-4534-bcae-ba1e3f79820e"
          },
          "dapAn": "Nathan? Are you OK?",
          "phienAm": "/ˈneɪθən/. /ɑːr juː əʊˈkeɪ/",
          "yNghia": "\nNathan? Con ổn chứ?",
          "normal": {
            "start": "00:00:01.453",
            "end": "00:00:03.324"
          },
          "slow": {
            "start": "00:00:02.264",
            "end": "00:00:06.450"
          }
        },
        {
          "_id": {
            "$oid": "7bfab273-4777-405a-9c0e-cd1ab936af3f"
          },
          "dapAn": "Where are you?",
          "phienAm": "/weər ɑːr juː/",
          "yNghia": "\nCon đang ở đâu?",
          "normal": {
            "start": "00:00:05.286",
            "end": "00:00:06.231"
          },
          "slow": {
            "start": "00:00:09.504",
            "end": "00:00:11.741"
          }
        },
        {
          "_id": {
            "$oid": "d8ff6aae-a294-455f-adcd-cdb98a6316c5"
          },
          "dapAn": "Why are you calling me?",
          "phienAm": "/waɪ ɑːr juː ˈkɔːlɪŋ miː/",
          "yNghia": "\nTại sao con lại gọi cho mẹ thế?",
          "normal": {
            "start": "00:00:08.250",
            "end": "00:00:09.592"
          },
          "slow": {
            "start": "00:00:14.743",
            "end": "00:00:17.191"
          }
        },
        {
          "_id": {
            "$oid": "7e0f075d-63e0-4341-93ec-1879e414b711"
          },
          "dapAn": "Why are you always so forgetful?",
          "phienAm": "/waɪ ɑːr juː ˈɔːlweɪz səʊ fəˈgetfl/",
          "yNghia": "\nTại sao con luôn quên vậy hả?",
          "normal": {
            "start": "00:00:12.499",
            "end": "00:00:14.751"
          },
          "slow": {
            "start": "00:00:21.772",
            "end": "00:00:26.458"
          }
        },
        {
          "_id": {
            "$oid": "4001545b-9796-4511-ab9b-acfa3e0cabd5"
          },
          "dapAn": "Where did you put it?",
          "phienAm": "/weər dɪd juː pʊt ɪt/",
          "yNghia": "\nCon đã để nó ở đâu?",
          "normal": {
            "start": "00:00:17.712",
            "end": "00:00:19.039"
          },
          "slow": {
            "start": "00:00:30.486",
            "end": "00:00:33.066"
          }
        },
        {
          "_id": {
            "$oid": "88742403-7be4-428b-9577-92c8ff555349"
          },
          "dapAn": "It's your bag! I don't know. Hold on. It isn't here. No, sorry! Yes! Here it is!",
          "phienAm": "/ɪts jɔːr bæg/. /aɪ dəʊnt nəʊ/. /həʊld ɒn/. /ɪt ˈɪznt hɪər/. /nəʊ ˈsɒri/. /jes/. /hɪər ɪt ɪz/",
          "yNghia": "\nĐó là cặp của con đó! Mẹ không biết. Chờ đã. Nó không có ở đây. Không, mẹ xin lỗi! Nó đây rồi!",
          "normal": {
            "start": "00:00:23.050",
            "end": "00:00:31.755"
          },
          "slow": {
            "start": "00:00:38.436",
            "end": "00:00:51.467"
          }
        },
        {
          "_id": {
            "$oid": "794689a5-4908-406d-abbc-d093fc3ab866"
          },
          "dapAn": "A lot of things.",
          "phienAm": "/eɪ lɒt ɒv θɪŋz/",
          "yNghia": "\nRất nhiều thứ.",
          "normal": {
            "start": "00:00:35.740",
            "end": "00:00:36.878"
          },
          "slow": {
            "start": "00:00:56.180",
            "end": "00:00:57.865"
          }
        },
        {
          "_id": {
            "$oid": "b924638b-5f6f-4fa1-8320-7156bb86aa7f"
          },
          "dapAn": "Yes, your laptop and ... your umbrella ... your sandwiches! Your lunch is in the bag!",
          "phienAm": "/jes jɔːr ˈlæptɒp ænd ... jɔːr ʌmˈbrelə ... jɔːr ˈsænwɪʤɪz/. /jɔːr lʌnʧ ɪz ɪn ðə bæg/",
          "yNghia": "\nCó, máy tính xách tay và... chiếc ô... bánh mì kẹp của con! Bữa trưa của con có trong cặp!",
          "normal": {
            "start": "00:00:39.930",
            "end": "00:00:47.487"
          },
          "slow": {
            "start": "00:01:01.682",
            "end": "00:01:11.870"
          }
        },
        {
          "_id": {
            "$oid": "fad9184e-b6be-4ef7-8f1b-3cc444907352"
          },
          "dapAn": "And two notebooks ... and your wallet! Nathan! Everything you need is in your bag!",
          "phienAm": "/ænd tuː ˈnəʊtbʊks ... ænd jɔːr ˈwɒlɪt/. /ˈneɪθən/. /ˈevrɪθɪŋ juː niːd ɪz ɪn jɔːr bæg/",
          "yNghia": "\nVà hai cuốn sổ... và ví tiền của con! Nathan! Mọi thứ con cần đều có trong cặp của con!",
          "normal": {
            "start": "00:00:50.678",
            "end": "00:00:57.643"
          },
          "slow": {
            "start": "00:01:15.898",
            "end": "00:01:24.744"
          }
        },
        {
          "_id": {
            "$oid": "2d224a74-265c-42ce-84e9-b690f206e3eb"
          },
          "dapAn": "I'm on my way to school now!",
          "phienAm": "/aɪm ɒn maɪ weɪ tuː skuːl naʊ/",
          "yNghia": "\nMẹ đang trên đường đến trường bây giờ!",
          "normal": {
            "start": "00:01:00.549",
            "end": "00:01:02.493"
          },
          "slow": {
            "start": "00:01:27.929",
            "end": "00:01:30.930"
          }
        }
      ]
    }
