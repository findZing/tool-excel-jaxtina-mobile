          "yNghia": "\nMẹ có thấy ví tiền của con không?",
          "normal": {
            "start": "00:00:48.323",
            "end": "00:00:49.940"
          },
          "slow": {
            "start": "00:01:13.028",
            "end": "00:01:14.924"
          }
        },
