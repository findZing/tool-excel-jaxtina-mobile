          "phienAm": "/θæŋks mɒm/. /ˈsɒri .../",
          "yNghia": "\nCảm ơn mẹ! Con xin lỗi ạ ...",
          "normal": {
            "start": "00:00:58.224",
            "end": "00:00:59.859"
          },
          "slow": {
            "start": "00:01:25.533",
            "end": "00:01:27.560"
          }
        }
