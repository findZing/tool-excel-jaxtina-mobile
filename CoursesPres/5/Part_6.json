      "cauHoi": "<table>\n        <tbody>\n                <tr>\n                        <td><b>Joy:</b></td>\n                        <td>Hi. What's <b>(1)</b> <b>_____</b> name?</td>\n                </tr>\n                <tr>\n                        <td><b>Alex:</b></td>\n                        <td>My name <b>(2) _____</b> Alex Green.</td>\n                </tr>\n                <tr>\n                        <td><b>Maria:</b></td>\n                        <td><b>(3)</b> <b>_____</b> to meet you, Alex. How old <b>(4)</b> <b>_____</b> you?</td>\n                </tr>\n                <tr>\n                        <td><b>John:</b></td>\n                        <td>I'm 24</td>\n                </tr>\n        </tbody>\n</table>",
      "yNghia": "<table>\r\n        <tbody>\r\n                <tr>\r\n                        <td><b>Joy:</b></td>\r\n                        <td>Hi. What's <b>(1)</b> <span style=\"color:red\"><b>your</b></span>  name?</td>\r\n                </tr>\r\n                <tr>\r\n                        <td> </td>\r\n                        <td><i>Xin chào. Tên <span style=\"color:red\">của em</span>  là gì? </i></td>\r\n                </tr>\r\n                <tr>\r\n                        <td><b>Alex:</b></td>\r\n                        <td>My name <b>(2) <span style=\"color:red\">is</span> </b> Alex Green.</td>\r\n                </tr>\r\n                <tr>\r\n                        <td> </td>\r\n                        <td><i>Tên em <span style=\"color:red\">là</span>  Alex Green ạ.</i></td>\r\n                </tr>\r\n                <tr>\r\n                        <td style=\"vertical-align: top;\"><b>Maria:</b></td>\r\n                        <td><b>(3)</b> <span style=\"color:red\"><b>Nice</b></span>  to meet you, Alex. How old <b>(4)</b> <b><span style=\"color:red\">are</span> </b> you?</td>\r\n                </tr>\r\n                <tr>\r\n                        <td> </td>\r\n                        <td><i><span style=\"color:red\">Rất vui </span> được gặp em, Alex. Em bao nhiêu tuổi?</i></td>\r\n                </tr>\r\n                <tr>\r\n                        <td><b>John:</b></td>\r\n                        <td>I'm 24</td>\r\n                </tr>\r\n                <tr>\r\n                        <td> </td>\r\n                        <td><i>Em 24 tuổi .</i></td>\r\n                </tr>\r\n        </tbody>\r\n</table>",
